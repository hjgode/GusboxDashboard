{
  "manifest_version": 2,
  "name": "Show Gusbox Dashboard at this IP in new Tab",
  "version": "1.0.1",
 
  "description": "Presents a right-click menu option load selected IP in new tab.",
  "icons": {
    "48": "icons/logo.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "gusbox-dashboard-tab@example.com",
      "strict_min_version": "42.0",
      "update_url": "https://github.com/hjgode/GusboxDashboard"
    }
  },

  "background": {
    "scripts": ["background.js"]
  },
 
  "permissions": ["contextMenus"]
}

